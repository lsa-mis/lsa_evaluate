<div id="<%= dom_id entry %>">
  <p>
    <strong>Title:</strong>
    <%= entry.title %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= entry.status_id %>
  </p>

  <p>
    <strong>Contest instance:</strong>
    <%= entry.contest_instance_id %>
  </p>

  <p>
    <strong>Profile:</strong>
    <%= entry.profile_id %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= entry.category_id %>
  </p>

  <p>
    <strong>Attached file:</strong>
    <% if @entry.entry_file.attached? %>
      <%= link_to 'Download File', rails_blob_path(@entry.entry_file, disposition: "attachment") %>
    <% else %>
      No file attached
    <% end %>
  </p>

</div>
